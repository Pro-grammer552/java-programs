package com.prep;

public class RemoveExtraSpaces {

	public static void main(String[] args) {
		String s = "  i am  suraj";
		String n = s.trim();

		String p = "";
		// String rep = n.replace(" ", " ");

		// System.out.println(rep);
		String a = "";
		char ch[] = n.toCharArray();

		for (int i = 0; i < ch.length; i++) {
			if (ch[i] == ' ') {
				continue;
			} else {
				p += ch[i];
			}
			a = p + ' ';
		}

		System.out.println(a);

	}

}
