
public class aa {

	public static void main(String[] args) {
		int[] arr = { 10, 20, 20, 10, 10, 30, 50, 10, 20 };
		int num = 9;

		int count = 1;
		for (int i = 0; i < arr.length; i++) {
			for (int j = i + 1; j < arr.length; j++) {
				if (arr[i] == arr[j]) {
					count++;

				}

			}
			System.out.println(arr[i] + " " + count);
		}

	}

}
