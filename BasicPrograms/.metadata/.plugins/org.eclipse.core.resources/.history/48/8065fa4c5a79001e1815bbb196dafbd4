package com.prep;

import java.util.Arrays;

public class RotateArray2 {

	public static void main(String[] args) {
		int k = 2;
		int arr[] = {1, 2, 3, 4, 5, 6};
		int temp[] = new int[arr.length];

		int n = arr.length - 1;

		for (int i = 0; i < arr.length; i++) {

			temp[i] = n;

		}

		System.out.println(Arrays.toString(temp));

	}

}
