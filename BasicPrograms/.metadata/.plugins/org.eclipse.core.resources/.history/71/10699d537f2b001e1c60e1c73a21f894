package com.Assignment53Q;

/*
 6.	WAP to take array and print prime numbers
 */

public class PrimeNumber {

	public static void main(String[] args) {

		int flag = 1;
		int n = 10;
		for (int i = 0; i < n; i++) {

			if (n % i == 0) {
				flag++;
				break;

			}

			if (flag == 1) {
				System.out.println("prime");
			} else {
				System.out.println("non prime");
			}
		}

	}

}
