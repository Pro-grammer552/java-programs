package com.prep;

public class AcendingString {

	public static void main(String[] args) {

		String s = "suraj";
		for (int i = 0; i < s.length(); i++) {
			boolean flag = false;
			for (int j = i + 1; j < s.length(); j++) {
				// char ch = s.charAt(i);

				if (s.charAt(i) > s.charAt(j)) {

					flag = true;
				}

			}

			if (flag = true) {
				System.out.println("Ascending");
			} else {
				System.out.println("unordered");
			}
		}

	}

}
