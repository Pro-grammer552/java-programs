package com.prep;

import java.util.Arrays;

public class AscendingDesending {

	public static void main(String[] args) {

		int arr[] = {4, 5, 3, 2, 6, 4};

		for (int i = 0; i < arr.length; i++) {
			for (int j = i + 1; j < arr.length; j++) {
				if (arr[i] > arr[j]) {
					int temp = arr[i];
					arr[i] = arr[j];
					arr[i] = temp;
				}
				System.out.println(Arrays.toString(arr));
			}
		}

	}

}
