package com.prep;

public class UniqueElement {

	public static void main(String[] args) {
		int[] arr = { 0, 0, 30, 40, 50, 50, 50 };
		
		int arr1[]=new int[arr.length];
		
		for(int i=0;i<arr.length;i++)
		{
			int j=i+1;
			if(arr[i]!=arr[j] && i!=j)
			{
				arr1[i]=arr[i];
			}
			j++;
			
			System.out.println(arr[i]);
		}

	}

}
