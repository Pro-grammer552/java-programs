package com.Stringassignmet40;

public class ReverseString {
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		String str="I love My India";
		String rev="";
		
		for(int i=str.length()-1;i<=0;i--)
		{
			
			rev=str+rev;
			
		}
		System.out.println(rev);
		
	}

}
