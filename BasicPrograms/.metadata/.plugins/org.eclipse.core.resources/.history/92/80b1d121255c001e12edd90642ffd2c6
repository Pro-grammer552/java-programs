package com.String_Basic;

public class reverseInteger {

	public static void main(String[] args) {

		int a = -123;
		StringBuilder s = new StringBuilder();
		s.append(Math.abs(a));
		s.reverse();

		System.out.println(s.toString());

		if (a < 0) {
			//s.insert(0, '-');
			a=a*-1;
			
			
		}
		
		System.out.println(s.toString());

	}

}
