package com.prep;

public class AvgOfWord {

	public static void main(String[] args) {
		String s = "sachin scores 89 runs";

		String arr[] = s.split(" ");

		String ne = "";

		for (int i = 0; i < s.length(); i++) {
			char ch = s.charAt(i);

			if (Character.isDigit(ch)) {
				ne += ch;
			}

			System.out.println(ne);

		}

	}

}
